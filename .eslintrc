{
	"parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-shadow": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ],
    "import/prefer-default-export": 0,
    "no-nested-ternary": 0
  },
  "env": {
    "browser": true
  },
  "globals": {
    "chrome": false
  }
}
